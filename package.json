{
  "name": "staticify",
  "version": "4.0.0",
  "description": "A better static asset handler for Node.js/Express.js",
  "main": "index.js",
  "author": "Rakesh Pai <rakeshpai@errorception.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/errorception/staticify.git"
  },
  "scripts": {
    "mocha": "mocha --exit",
    "xo": "xo",
    "test": "npm run xo && npm run mocha",
    "test:cov": "nyc npm run test"
  },
  "keywords": [
    "static",
    "asset",
    "versioning",
    "caching",
    "express",
    "expressjs"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "ignore-by-default": "^1.0.1",
    "memoizee": "^0.4.14",
    "send": "^0.17.1"
  },
  "devDependencies": {
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "xo": "^0.30.0"
  },
  "engines": {
    "node": ">=10"
  },
  "xo": {
    "space": 4,
    "rules": {
      "capitalized-comments": "off",
      "spaced-comment": "off",
      "func-style": [
        "error",
        "expression"
      ],
      "unicorn/better-regex": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/string-content": "off"
    },
    "overrides": [
      {
        "files": "test/**",
        "envs": [
          "mocha"
        ]
      }
    ]
  },
  "nyc": {
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ]
  }
}
